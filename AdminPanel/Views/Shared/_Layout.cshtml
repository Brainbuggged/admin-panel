<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Админская панель</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/jsonDataTable.js"></script>
    <script src="~/js/createDataTables.js"></script>
    <script src="~/DataTables/datatables.min.js"></script>
    <script src="~/DataTables/DataTables-1.11.3/js/jquery.dataTables.min.js"></script>
    <link rel="stylesheet" href="~/DataTables/datatables.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="~/assets/css/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/sass.css">
    <link rel="stylesheet" href="~/assets/css/layets.css">
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="~/assets/css/responsive.css">
</head>
    <body>
        <div class="main-wrapper">
            <!-- --NAVBAR---- -->
            <div class="header-container fixed-top">
                <header class="header  navbar navbar-expand-sm expand-header">
                    <div class="header-left d-flex">
                        <div class="logo">
                            BuDEVICE
                        </div>
                    </div>
                    <ul class="navbar-item flex-row ml-auto">
                        <li class="nav-item dropdown user-profile-dropdown">
                            <a href="" class="nav-link user" id="Notify" data-bs-toggle="dropdown">
                                <img src="assets/img/bell.svg" alt="" class="icon">
                                <p class="count purple-gradient ">5</p>
                            </a>
                            <div class="dropdown-menu">
                                <div class="dp-main-menu">
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/server.svg" alt="" class="user-note">
                                        <div class="note-info-desmis">
                                            <div class="user-notify-info">
                                                <p class="note-name">Server Rebooted</p>
                                                <p class="note-time">20 min ago</p>
                                            </div>
                                            <p class="status-link"><span class="fas fa-times"></span></p>
                                        </div>
                                    </a>


                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/heart.svg" alt="" class="user-note">
                                        <div class="note-info-desmis">
                                            <div class="user-notify-info">
                                                <p class="note-name">Love your work</p>
                                                <p class="note-time">22 min ago</p>
                                            </div>
                                            <p class="status-link"><span class="fas fa-times"></span></p>
                                        </div>
                                    </a>
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/note.svg" alt="" class="user-note">
                                        <div class="note-info-desmis">
                                            <div class="user-notify-info">
                                                <p class="note-name">Helal Sent a File</p>
                                                <p class="note-time">25 min ago</p>
                                            </div>
                                            <p class="status-link"><span class="fas fa-times"></span></p>
                                        </div>
                                    </a>
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/error.svg" alt="" class="user-note">
                                        <div class="note-info-desmis">
                                            <div class="user-notify-info">
                                                <p class="note-name">Server Don't work!</p>
                                                <p class="note-time">30 min ago</p>
                                            </div>
                                            <p class="status-link"><span class="fas fa-times"></span></p>
                                        </div>
                                    </a>
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/note.svg" alt="" class="user-note">
                                        <div class="note-info-desmis">
                                            <div class="user-notify-info">
                                                <p class="note-name">Helal Sent a File</p>
                                                <p class="note-time">25 min ago</p>
                                            </div>
                                            <p class="status-link"><span class="fas fa-times"></span></p>
                                        </div>
                                    </a>
                                </div>
                            </div>


                        </li>
                        <li class="nav-item dropdown user-profile-dropdown">
                            <a href="" class="nav-link user" id="Notify" data-bs-toggle="dropdown">
                                <img src="assets/img/envelope.svg" alt="" class="icon">
                                <p class="count bg-clc">5</p>
                            </a>

                            <div class="dropdown-menu">
                                <div class="dp-main-menu">
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/user-1.jpeg" alt="" class="sms-user">
                                        <div class="user-message-info">
                                            <p class="m-user-name">Helal Uddin</p>
                                            <p class="user-role">Super Admin</p>
                                        </div>
                                    </a>
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/user-3.png" alt="" class="sms-user">
                                        <div class="user-message-info">
                                            <p class="m-user-name">Admin Sohel</p>
                                            <p class="user-role">Admin</p>
                                        </div>
                                    </a>
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/user-2.jpg" alt="" class="sms-user">
                                        <div class="user-message-info">
                                            <p class="m-user-name">Shinthia Kabir</p>
                                            <p class="user-role">Co-Admin</p>
                                        </div>
                                    </a>
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/user-4.png" alt="" class="sms-user">
                                        <div class="user-message-info">
                                            <p class="m-user-name">Jewel Rana</p>
                                            <p class="user-role">Writer</p>
                                        </div>
                                    </a>
                                    <a href="" class="dropdown-item message-item">
                                        <img src="assets/img/user-5.jpg" alt="" class="sms-user">
                                        <div class="user-message-info">
                                            <p class="m-user-name">Raisa Khan</p>
                                            <p class="user-role">Admin</p>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        </li>

                        <li class="nav-item dropdown user-profile-dropdown">
                            <a href="" class="nav-link user" id="Notify" data-bs-toggle="dropdown">
                                <img src="assets/img/profile.svg" alt="" class="icon">
                            </a>

                            <div class="dropdown-menu">
                                <div class="user-profile-section">
                                    <div class="media mx-auto">
                                        <img src="assets/img/profile.svg" alt="" class="img-fluid mr-2">
                                        <div class="media-body">
                                            <h5>Helal Uddin</h5>
                                            <p>Super admin</p>
                                        </div>
                                    </div>
                                </div>

                                <div class="dp-main-menu">
                                    <a href="" class="dropdown-item"><span class="fas fa-user"></span>Profile</a>
                                    <a href="" class="dropdown-item"><span class="fas fa-inbox"></span>Inbox</a>
                                    <a href="" class="dropdown-item"><span class="fas fa-lock-open"></span>Look Screen</a>
                                    <a href="" class="dropdown-item"><span class="fas fa-outdent"></span>Log Out</a>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item dropdown user-profile-dropdown">
                            <a href="" class="nav-link user" id="Notify" data-bs-toggle="dropdown">
                                <img src="assets/img/settings.svg" alt="" class="icon">
                            </a>

                            <div class="dropdown-menu">
                                <div class="dp-main-menu">
                                    <a href="" class="dropdown-item"><span class="fas fa-plug"></span>Parmition</a>
                                    <a href="" class="dropdown-item"><span class="fas fa-users"></span>Admins</a>
                                    <a href="" class="dropdown-item"><span class="fas fa-object-ungroup"></span>Design Type</a>
                                    <a href="" class="dropdown-item"><span class="fas fa-palette"></span>Color</a>
                                </div>
                            </div>
                        </li>
                    </ul>
                </header>
            </div>
            <!-- --NAVBAR END---- -->
            <!-- ------------------SIDEBAR START----------- -->
            <div class="left-menu">
                <div class="menubar-content">
                    <nav class="animated bounceInDown">
                        <ul id="sidebar">
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="ProductModels" asp-route-is_checked="false" asp-action="Index">Товары, ожидающие модерации</a>
                            </li>
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="ProductCategoryModels" asp-action="Index">Категории товаров</a>
                            </li>
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="ParameterModels" asp-action="Index">Параметры категорий</a>
                            </li>
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="ParameterValueModels" asp-action="Index">Значения параметров</a>
                            </li>
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="ClientModels" asp-action="Index">Клиенты</a>
                            </li>
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="VendorModels" asp-action="Index">Продавцы</a>
                            </li>
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="ProductModels" asp-route-is_checked="true" asp-action="Index">Все товары</a>
                            </li>
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="OrderModels" asp-action="Index">Заказы</a>
                            </li>
                            <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Logs">Логи</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <!-- ------------------SIDEBAR END----------- -->
            <br />
            <br />
            <div class="container" style="margin-left:240px">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>
        <script src="assets/js/main.js"></script>

        <footer class="border-top footer text-muted">
            @* <div class="container"> *@
            @*     &copy; 2021 - AdminPanel - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a> *@
            @* </div> *@
        </footer>


        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>